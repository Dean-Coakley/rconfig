{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^3.3",
    "enlightn/enlightn": "^2.3",
    "fakerphp/faker": "^1.19",
    "guzzlehttp/guzzle": "^7.0.1",
    "http-interop/http-factory-guzzle": "^1.0",
    "laravel/framework": "^10.0",
    "laravel/horizon": "^5.7",
    "laravel/passport": "v11.8.0",
    "laravel/sanctum": "v3.2",
    "laravel/tinker": "^2.8",
    "laravel/ui": "v4.2.1",
    "lorisleiva/cron-translator": "^0.3.0",
    "markhilton/monolog-mysql": "^0.1.6",
    "nunomaduro/larastan": "^2.5",
    "phpseclib/phpseclib": "~3.0",
    "predis/predis": "v2.0.2",
    "qruto/laravel-flora": "^1.0",
    "spatie/cpu-load-health-check": "^1.0",
    "spatie/data-transfer-object": "^3.7",
    "spatie/laravel-activitylog": "^4.5",
    "spatie/laravel-health": "^1.8",
    "spatie/laravel-ignition": "^2.0",
    "symfony/yaml": "^6.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.12",
    "beyondcode/laravel-dump-server": "^1.8",
    "glhd/laravel-dumper": "^1.0",
    "laravel/pint": "v1.1.3",
    "mockery/mockery": "^1.5.0",
    "nunomaduro/collision": "^7.0",
    "phpunit/php-code-coverage": "^10.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Http/Helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
